	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">

	<bean id="sozlerBean" class="com.hacidoganilbars.Sozler">
		<property name="cumle"
			value="Güzel gören güzel düşünür. 
Güzel düşünen hayatından lezzet alır." />
	</bean>

	<bean id="baslatmaBean" class="com.hacidoganilbars.ContextOlayBaslatmaIsleyicisi" />
	<bean id="durdurmaBean" class="com.hacidoganilbars.ContextOlayDurdurmaIsleyicisi" />
	<bean id="customOlayIsleyicisiBean" class="com.hacidoganilbars.CustomOlayIsleyicisi" />
	<bean id="customOlayYayinlayicisiBean" class="com.hacidoganilbars.CustomOlayYayinlayicisi" />

</beans>
